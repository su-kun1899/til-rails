<h1>People#index</h1>
<p><%= @msg %></p>
<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th></th>
  </tr>
  <% @people.each do |person| %>
      <tr>
        <td><%= person.id %></td>
        <td><a href="/people/<%= person.id %>"><%= person.name %></a></td>
        <td><a href="/people/edit/<%= person.id %>">Edit</a></td>
      </tr>
  <% end %>
</table>